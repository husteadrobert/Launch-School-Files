<body>
<ul>
  <li>Name: <%= @name %></li>
  <li>Email Address: <%= @email_address %></li>
  <li>Interests: <%= @interests %></li>
</ul>


<h3>Links</h3>
<ul>
  <% @name_list.each do |other_name| %>
    <% next if other_name == @name %>
    <li><a href="/user/<%= other_name %>"><%= other_name %></a></li>
  <% end %>
</ul>
</body>